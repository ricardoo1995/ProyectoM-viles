        android:layout_marginStart="16dp"
        android:text="Nombre"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txtShowNombreEstudiante"
        app:layout_constraintEnd_toStartOf="@+id/txtShowNombreEstudiante"
        app:layout_constraintHorizontal_bias="0.421"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtShowNombreEstudiante"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Apellido"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txtShowApellidoEstudiante"
        app:layout_constraintEnd_toStartOf="@+id/txtShowApellidoEstudiante"
        app:layout_constraintHorizontal_bias="0.485"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtShowApellidoEstudiante"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
        android:text="Fecha Nac"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txtShowFechaNacEstudiante"
        app:layout_constraintEnd_toStartOf="@+id/txtShowFechaNacEstudiante"
        app:layout_constraintHorizontal_bias="0.558"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtShowFechaNacEstudiante"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
        android:text="Semestre"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/txtShowSemestreEstudiante"
        app:layout_constraintEnd_toStartOf="@+id/txtShowSemestreEstudiante"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/txtShowSemestreEstudiante"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="16dp"
